<clickhouse replace="true">
    <user_directories>
        <users_xml>
            <path>users.xml</path>
        </users_xml>
        <local_directory>
            <path>/var/lib/clickhouse/access/</path>
        </local_directory>
    </user_directories>
    <timezone>America/Chicago</timezone>
    <zookeeper>
        <node>
            <host>zookeeper</host>
            <port>2181</port>
        </node>
    </zookeeper>
    <macros>
        <replica>clickhouse</replica>
        <shard>02</shard>
    </macros>
</clickhouse>